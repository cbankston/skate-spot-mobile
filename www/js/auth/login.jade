ion-view(view-title="Login")  ion-content.padding    form(name="loginForm" ng-submit="loginForm.$valid && login_skater(skater)" novalidate="")      .list        label.item.item-input          input(type='text', placeholder='Email Address' ng-model="skater.email")        label.item.item-input          input(type='password', placeholder='Password', ng-model="skater.password")      input(type="submit", class="button button-block button-positive activated", value="Login")